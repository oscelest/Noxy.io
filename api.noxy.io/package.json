{
  "name": "noxy.io-server",
  "version": "1.0.0",
  "description": "Main API Server.",
  "scripts": {
    "start": "ts-node ./app.ts",
    "dev": "nodemon ./app.ts",
    "update": "npm update && npm outdated"
  },
  "nodemonConfig": {
    "watch": [
      "./"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "500ms",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.19",
    "@types/fs-extra": "^9.0.10",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/method-override": "0.0.31",
    "@types/mime-types": "^2.1.0",
    "@types/multer": "^1.4.5",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.14.37",
    "@types/serve-favicon": "^2.5.2",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.11.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "fs-extra": "^9.1.0",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "mime-types": "^2.1.30",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "nanoid": "^3.1.22",
    "serve-favicon": "^2.5.0",
    "ts-node": "^9.1.1",
    "typeorm": "^0.2.32",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.1"
  }
}
