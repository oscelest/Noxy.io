{
  "name": "@noxy.io/backend",
  "version": "1.0.0",
  "description": "Main API Server.",
  "scripts": {
    "start": "ts-node ./app.ts",
    "dev": "nodemon ./app.ts",
    "update": "npm update && npm outdated"
  },
  "nodemonConfig": {
    "watch": [
      "./"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "500ms",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.30",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "nanoid": "^3.1.23",
    "ts-node": "^10.0.0",
    "typeorm": "^0.2.32",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.19",
    "@types/fs-extra": "^9.0.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.170",
    "@types/mime-types": "^2.1.0",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^15.6.0",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.13"
  }
}
